node('maven'){
stage('first stage')
{
  // use the id of the globally configured maben instance
  
  def mvnTool = tool 'Apache Maven 3.6.2'
  
  //execute maven
  sh "${mvnTool}/bin/mvn clean install"
  sh "echo hello world"
 } 
 
 stage('2nd stage') 
 {
  sh "touch helloworld.txt"
  }
  
  stage('3rd stage')
  {
  echo"mkdir bamdev"
  }
 } 
